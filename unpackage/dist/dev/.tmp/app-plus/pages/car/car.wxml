<view class="car_view data-v-00adb888"><view class="car_list_box data-v-00adb888"><block wx:for="{{cart_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="car_item data-v-00adb888"><label data-event-opts="{{[['tap',[['changeRadio',[index]]]]]}}" class="radio data-v-00adb888" bindtap="__e"><radio value="{{item.value}}" color="#fbc800" checked="{{item.current==1}}" class="data-v-00adb888"></radio></label><view class="car_info data-v-00adb888"><view class="ci_box data-v-00adb888"><image class="ci_img data-v-00adb888" src="{{item.src}}" mode="widthFix"></image><view class="c_info_box data-v-00adb888"><view class="cib_title data-v-00adb888">{{item.title}}</view><view class="cib_info data-v-00adb888">{{item.info}}</view><uni-number-box vue-id="{{'1-'+index}}" min="{{0}}" value="{{item.num}}" max="{{99}}" class="data-v-00adb888" bind:__l="__l"></uni-number-box></view></view><image class="ci_del data-v-00adb888" src="../../static/delete1.jpg" mode="widthFix" data-event-opts="{{[['tap',[['toDelCar',['$0'],[[['cart_list','',index,'id']]]]]]]}}" catchtap="__e"></image></view></view></block></view><view class="fix_car_bottom data-v-00adb888"><label data-event-opts="{{[['tap',[['allRadio',['$event']]]]]}}" class="radio data-v-00adb888" bindtap="__e"><radio value="" color="#fbc800" checked="{{isAll==1}}" class="data-v-00adb888"></radio>全选</label><view class="fix_car_info data-v-00adb888"><text class="data-v-00adb888">总计：￥888.88</text><button data-event-opts="{{[['tap',[['toAccount',['$event']]]]]}}" bindtap="__e" class="data-v-00adb888">结算</button></view></view></view>